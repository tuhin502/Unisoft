@model AN.Entities.ViewModel.GetOrderListVM
@{
    ViewBag.Title = "ProductDetails";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style>
    th {
        color: white;
    }

    td {
        text-align: center;
    }

    #od {
        color: black;
    }
</style>
<link href="~/Content/Registration/orderdetails.css" rel="stylesheet" />

<h3 class="text-center p-3 text-white" style="background:#023e8a;">Order List</h3>
<div class="container-fluid mb-3" id="ordershow">


    <div class="container">
        <div class="row">

            <div class="col-md-12 col-sm-12 col-lg-12">
                <div class="serviceBox green">


                    <div class="service-content">
                        @*<h5><b>Your Order</b></h5>*@
                        <p class="description">
                            <ul class="list-group">
                                <li class="list-group-item"><span id="od">Order Id : </span>@Model.o_ID</li>
                                <li class="list-group-item"><span id="od">Product Name : </span>@Model.prdt_Name</li>
                                <li class="list-group-item"><span id="od">Package Name : </span>@Model.pkg_Name</li>
                                <li class="list-group-item"><span id="od">OrderDate : </span>@Model.entryDate</li>
                                <li class="list-group-item"><span id="od">OrderBy : </span>@Model.userBy</li>
                                <li class="list-group-item"><span id="od">Paymentstatus : </span>@Model.paymentSta Done</li>

                            </ul>
                        </p>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>


